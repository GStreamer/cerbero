prefix=/usr
exec_prefix=${prefix}
libdir=${prefix}/lib
includedir=${prefix}/include

gmodule_supported=true

Name: GModule
Description: Dynamic module loader for GLib
Requires: glib-2.0
Version: 2.30.0
Libs: -L${libdir} -Wl,--export-dynamic -lgmodule-2.0 -pthread  -lrt
Libs.private: -ldl
Cflags: -pthread
